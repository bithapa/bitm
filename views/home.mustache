    <div id="vertex-container" class="wrapper">
        <div class="sidebar-wrapper">
            <div class="profile-container">
                <img class="profile" style="border-radius:90%" src="https://media-exp1.licdn.com/dms/image/C5603AQFC2UFLTdkaoA/profile-displayphoto-shrink_200_200/0?e=1588204800&v=beta&t=C_fDqCFe4Lu3YU6zedeSGp-KJ4QCVnMhMUOmk-xoUQo" alt="Biksah Thapa" />
                <h1 class="name">{{page.firstName}} {{page.lastName}}</h1>
                <h3 class="tagline">{{page.occupation}}</h3>
            </div>

            <div class="contact-container container-block">
                <ul class="list-unstyled contact-list">
                    <li class="email"><i class="fas fa-envelope"></i><a href="mailto: {{page.email}}">{{page.email}}</a></li>

                    <!-- <li class="website"><i class="fas fa-globe"></i><a href="https://www.google.com" target="_blank">google.com</a></li> -->

                    <li class="linkedin"><i class="fab fa-linkedin-in"></i><a href="{{page.linkedin_link}}" target="_blank">{{page.linkedin_username}}</a></li>

                    <li class="github"><i class="fab fa-github"></i><a href="{{page.github_link}}" target="_blank">{{page.github_username}}</a></li>

                    <li class="twitter"><i class="fab fa-twitter"></i><a href="{{page.twitter_link}}" target="_blank">{{page.twitter_username}}</a></li>

                    <!-- <li class="dribble"><i class="fab fa-dribbble"></i><a href="https://www.dribbble.com/" target="_blank">my_dribbble_username</a></li> -->

                    <!-- <li class="youtube"><i class="fab fa-youtube"></i><a href="https://www.youtube.com/" target="_blank">my_youtube_username</a></li> -->

                    <li class="instagram"><i class="fab fa-instagram"></i><a href="{{page.instagram_link}}" target="_blank">{{page.instagram_username}}</a></li>

                    <!-- <li class="pinterest"><i class="fab fa-pinterest"></i><a href="https://www.pinterest.com/" target="_blank">my_pinterest_username</a></li> -->
                </ul>
            </div>

            <div class="education-container container-block">
                <h2 class="container-block-title">Education</h2>

